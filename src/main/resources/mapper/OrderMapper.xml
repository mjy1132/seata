<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cx.seata_demo.mapper.OrderMapper">

   <insert id="save" parameterType="com.cx.seata_demo.entity.Order">
       insert into t_order
       (id,user_id,remarks,amount)
       values (#{id},#{userId},#{remarks},#{amount})
   </insert>
</mapper>